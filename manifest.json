{
  "name": "New Tab Override",
  "version": "1.0",
  "manifest_version": 3,
  "description": "This is to override new tab default page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": {            
      "16": "icons8-vimeo-16.png", 
      "24": "icons8-vimeo-16.png", 
      "32": "icons8-vimeo-16.png"  
    },
    "default_title": "Click to open control panel",
    "default_popup": "popup.html"
  },
  "icons": {            
      "16": "icons8-vimeo-16.png", 
      "24": "icons8-vimeo-16.png", 
      "32": "icons8-vimeo-16.png"  
    }
}